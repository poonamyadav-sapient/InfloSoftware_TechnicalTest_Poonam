@model UserManagement.Web.Models.Log.LogListViewModel


<div class="table-responsive">
	<h2>Log List</h2>
	@if (Model.logItems != null && Model.logItems.Any())
	{
		<table class="table table-striped">
			<thead>
				<tr>
					<th>ID</th>
					<th>Timestamp</th>
					<th>Action Type</th>
					<th>User ID</th>
					<th>Details</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var log in Model.logItems)
				{
					<tr>
						<td>@log.Id</td>
						<td>@log.Timestamp</td>
						<td>@log.ActionType</td>
						<td>@log.UserId</td>
						<td>@log.Details</td>
					</tr>
				}
			</tbody>

		</table>
	}
	else
	{
		<p>No Logs Found</p>
	}

</div>

